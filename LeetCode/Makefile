.PHONY: clean All

All:
	@echo "----------Building project:[ 2_AddTwoNumbers - Debug ]----------"
	@cd "2_AddTwoNumbers" && $(MAKE) -f  "2_AddTwoNumbers.mk"
clean:
	@echo "----------Cleaning project:[ 2_AddTwoNumbers - Debug ]----------"
	@cd "2_AddTwoNumbers" && $(MAKE) -f  "2_AddTwoNumbers.mk" clean
